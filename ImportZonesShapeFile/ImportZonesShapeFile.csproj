<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <OutDir>bin\$(Configuration)\$(TargetFramework)</OutDir>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningsAsErrors />
    <LangVersion>latest</LangVersion>
    <RootNamespace>Geotab.SDK.ImportZonesShapeFile</RootNamespace>
    <ServerGarbageCollection>true</ServerGarbageCollection>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="Geotab.Geographical">
      <HintPath>..\Geotab.Geographical.dll</HintPath>
    </Reference>
    <Reference Include="Geotab.Geographical.MapLayers">
      <HintPath>..\Geotab.Geographical.MapLayers.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="CoreCompat.System.Drawing.v2" Version="5.2.0-preview1-r131" />
    <PackageReference Include="Geotab.Checkmate.ObjectModel" Version="11.7.179" />
  </ItemGroup>
</Project>